{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/webhook.js"
    },
    {
      "src": "/api/webhook/poll",
      "methods": ["GET", "OPTIONS"],
      "dest": "/api/webhook.js"
    },
    {
      "src": "/api/webhook/health",
      "methods": ["GET"],
      "dest": "/api/webhook.js"
    }
  ]
}
